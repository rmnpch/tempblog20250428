<%- include('partials/head', {title: 'Blog Authors'}) %>

<body>
    <h1>Blog Collaborating Authors</h1>
    <%- include('partials/nav') %>
    <% authors.forEach(author => { %>

            <a class="author-info" href="/author/<%=author.id  %>">
                <img src="<%= author.image %>" alt="<%= author.name %>">
                <h2 class="post-list">
                    <%= author.name %>
                </h2>
            </a>

    <% }) %>
</body>

</html>